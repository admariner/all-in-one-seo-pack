{
	"name": "all-in-one-seo-pack",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build:link-format-lite": "npm --prefix ./src/react/link-format run build-webpack-lite",
		"build:link-format-pro": "npm --prefix ./src/react/link-format run build-webpack-pro",
		"build:lite": "npm run env:lite && vite build && npm run build:link-format-lite",
		"build:pro": "npm run env:pro && vite build && npm run build:link-format-pro",
		"dev:lite": "npm run env:lite && vite",
		"dev:pro": "npm run env:pro && vite",
		"env:lite": "replace-in-files --string='VITE_VERSION=\"Pro\"' --replacement='VITE_VERSION=\"Lite\"' ./build/.env",
		"env:pro": "replace-in-files --string='VITE_VERSION=\"Lite\"' --replacement='VITE_VERSION=\"Pro\"' ./build/.env"
	},
	"engines": {
		"node": ">= 8.0.0"
	},
	"dependencies": {
		"@wordpress/autop": "3.3.1",
		"@wordpress/block-editor": "8.2.0",
		"@wordpress/editor": "12.2.2",
		"@wordpress/i18n": "4.3.1",
		"animate-vanilla-js": "1.0.10",
		"clipboard": "2.0.10",
		"js-base64": "3.7.2",
		"lodash-es": "4.17.21",
		"maz-ui": "1.3.1",
		"moment": "2.29.2",
		"moment-timezone": "0.5.34",
		"quill": "1.3.7",
		"superagent": "7.1.1",
		"tokenizer2": "2.0.1",
		"vue": "2.6.14",
		"vue-material": "1.0.0-beta-15",
		"vue-multiselect": "2.1.6",
		"vue-popperjs": "2.3.0",
		"vue-router": "3.5.3",
		"vue-scrollto": "2.20.0",
		"vue2-daterange-picker": "0.6.8",
		"vuex": "3.4.0",
		"vuex-router-sync": "5.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-replace": "^4.0.0",
		"@vitejs/plugin-react": "^1.3.2",
		"@vue/eslint-config-standard": "^6.1.0",
		"classnames": "^2.3.1",
		"dotenv": "^16.0.1",
		"eslint": "^8.16.0",
		"eslint-plugin-vue": "^8.7.1",
		"gettext-extractor": "^3.5.4",
		"plural-forms": "^0.5.3",
		"pofile": "^1.1.3",
		"postcss": "^8.4.14",
		"postcss-rtlcss": "^3.6.3",
		"replace-in-files-cli": "^2.0.0",
		"rollup-plugin-copy": "^3.4.0",
		"rollup-plugin-output-manifest": "^2.0.0",
		"rollup-pluginutils": "^2.8.2",
		"sass": "^1.52.1",
		"source-map-support": "^0.5.21",
		"url": "^0.11.0",
		"vite": "2.9.5",
		"vite-plugin-copy": "^0.1.6",
		"vite-plugin-dynamic-publicpath": "1.1.1",
		"vite-plugin-eslint": "^1.6.1",
		"vite-plugin-live-reload": "^2.1.0",
		"vite-plugin-vue2": "^1.9.3",
		"vue-template-compiler": "^2.6.14"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/awesomemotive/aioseo.git"
	}
}
