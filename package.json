    "@vue/compiler-sfc": "3.3.4",
    "@vue/eslint-config-standard": "^8.0.1",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "gettext-extractor": "^3.8.0",
    "htm": "3.1.1",
    "plural-forms": "^0.5.3",
    "pofile": "^1.1.4",
    "postcss": "^8.4.31",
    "postcss-rtlcss": "^4.0.8",
    "replace-in-files-cli": "^2.2.0",
    "rollup-plugin-copy": "^3.5.0",
