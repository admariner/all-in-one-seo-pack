{
	"name": "all-in-one-seo-pack",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "vue-cli-service build",
		"build-all-lite": "npm run build-lite && npm run build-webpack-lite",
		"build-webpack-dev": "webpack-cli --config webpack/dev.config.js --mode development",
		"build-webpack-pro": "npx dotenv -e .env.wp.pro -- webpack-cli --config webpack/pro.config.js --mode production",
		"build-webpack-lite": "npx dotenv -e .env.wp.lite -- webpack-cli --config webpack/lite.config.js --mode production",
		"build-lite": "npm run clear:babel-cache && vue-cli-service build --mode lite",
		"clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
		"lint": "vue-cli-service lint --no-fix",
		"lint-fix": "vue-cli-service lint",
		"start": "npx dotenv -e .env.wp vue-cli-service serve --open --port=8082",
		"serve": "vue-cli-service serve",
		"start-lite": "npx dotenv -e .env.wp.lite vue-cli-service serve",
		"start-pro": "npx dotenv -e .env.wp.pro vue-cli-service serve",
		"start-webpack": "webpack-cli --config webpack/dev.config.js --mode development -w",
		"start-webpack-lite": "webpack-cli --config webpack/lite.config.js --mode development -w",
	},
	"dependencies": {
		"@wordpress/autop": "2.9.0",
		"@wordpress/babel-preset-default": "^6.2.0",
		"@wordpress/block-editor": "5.1.2",
		"@wordpress/i18n": "3.12.0",
		"animate-vanilla-js": "^1.0.10",
		"clipboard": "2.0.6",
		"core-js": "3.6.5",
		"he": "^1.2.0",
		"js-base64": "3.6.1",
		"maz-ui": "1.3.1",
		"moment": "^2.29.1",
		"moment-timezone": "0.5.31",
		"quill": "1.3.7",
		"sentiment": "5.0.2",
		"superagent": "5.2.2",
		"tokenizer2": "2.0.1",
		"vue": "2.6.14",
		"vue-material": "1.0.0-beta-13",
		"vue-multiselect": "2.1.6",
		"vue-popperjs": "2.3.0",
		"vue-router": "3.1.6",
		"vue-scrollto": "2.18.2",
		"vue2-daterange-picker": "0.5.1",
		"vuex": "3.4.0",
		"vuex-router-sync": "5.0.0"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.13",
		"@vue/cli-plugin-eslint": "~4.5.13",
		"@vue/cli-plugin-vuex": "~4.5.13",
		"@vue/cli-service": "~4.5.13",
		"@vue/eslint-config-standard": "^6.0.0",
		"@wordpress/babel-plugin-makepot": "^3.5.0",
		"@wordpress/dependency-extraction-webpack-plugin": "^3.2.0",
		"babel-eslint": "^10.1.0",
		"babel-plugin-component": "^1.1.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"compression-webpack-plugin": "^4.0.0",
		"dotenv": "^10.0.0",
		"dotenv-cli": "^4.0.0",
		"eslint": "^7.29.0",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.1.0",
		"eslint-plugin-standard": "^4.1.0",
		"eslint-plugin-vue": "^7.11.1",
		"mini-css-extract-plugin": "^1.6.0",
		"node-sass": "^4.14.1",
		"npm-run-all": "^4.1.5",
		"sass-loader": "^8.0.2",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"vue-template-compiler": "2.6.14",
		"webpack-cli": "^4.7.2",
		"webpack-fix-style-only-entries": "^0.6.1",
		"webpack-rtl-plugin": "^2.0.0"
	},
	"browserslist": [
		"> 1%",
		"last 2 version",
		"not ie <= 8",
		"not dead"
	],
	"lint-staged": {
		"*.{js,vue}": [
			"vue-cli-service lint",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/awesomemotive/aioseo.git"
	}
}
