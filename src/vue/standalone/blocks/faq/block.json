{
	"$schema": "https://schemas.wp.org/wp/5.8/block.json",
	"name": "aioseo/faq",
	"title": "AIOSEO - FAQ",
	"description": "Easily add an SEO-friendly Frequently Asked Question (FAQ) to your content.",
	"category": "text",
	"keywords": [
		"accordion",
		"answer",
		"faq",
		"frequently asked questions",
		"knowledge base",
		"question",
		"schema",
		"seo"
	],
	"textdomain": "all-in-one-seo-pack",
	"attributes": {
		"hidden": {
			"type": "boolean",
			"default": false
		},
		"question": {
			"type": "string",
			"selector": ".aioseo-faq-block-question"
		},
		"schemaBlockId": {
			"type":"string",
			"default":""
		},
		"tagName": {
			"type": "string",
			"default": "h3"
		}
	},
	"supports": {
		"html": false,
		"anchor": true,
		"align": [
			"wide",
			"full"
		],
		"color": {
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalDefaultControls": {
				"fontSize": true,
				"lineHeight": true
			}
		}
	}
}